{
  "name": "maplink-sdk",
  "version": "0.0.1",
  "description": "SDK library that provides easy access to Maplink's services.",
  "keywords": ["maplink", "sdk", "geocoding", "logistics"],
  "author": "Samuel Lucas <shemlouk@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": { "require": "./dist/index.d.ts", "import": "./dist/index.d.mts" },
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": ["/dist"],
  "scripts": {
    "build": "tsup",
    "prebuild": "pnpm run lint",
    "lint": "biome lint --write core modules utils index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@types/node": "^22.7.5",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20"
  }
}
